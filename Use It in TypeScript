hereasync function transferSuperAdmin(program: anchor.Program, newAdmin: PublicKey) {
  const [configPda] = await PublicKey.findProgramAddress(
    [Buffer.from("registry-config")],
    program.programId
  );

  const txSig = await program.methods
    .transferSuperAdmin(newAdmin)
    .accounts({
      registryConfig: configPda,
      superAdmin: program.provider.wallet.publicKey,
    })
    .rpc();

  console.log("ðŸ‘‘ Super admin transferred:", txSig);
}
